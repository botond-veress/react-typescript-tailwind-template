{
  "name": "react-typescript-tailwind-template",
  "description": "Web app react template with typescript and tailwindcss.",
  "keywords": [
    "react",
    "typescript",
    "tailwindcss"
  ],
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Botond Veress <botond.veress@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack serve",
    "lint": "eslint --max-warnings 0 --ext .tsx --ext .ts src/",
    "typecheck": "tsc --noEmit",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --profile --json > stats.json && webpack-bundle-analyzer --mode static stats.json dist"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/css-minimizer-webpack-plugin": "^3.0.2",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/webpack-dev-server": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.2",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "style-loader": "^3.3.0",
    "type-fest": "^2.3.4",
    "webpack": "^5.58.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  }
}
